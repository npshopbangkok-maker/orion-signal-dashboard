# OrionAI WebSocket Configuration
# Use production OrionAI WebSocket for real signals
VITE_SIGNAL_WS_URL=wss://api.orion-ai.com/ws/signals

# For local development/testing, use demo mode by leaving empty
# VITE_SIGNAL_WS_URL=

# Telegram Notifications (Optional)
# Get bot token from @BotFather and chat ID from your bot
VITE_TELEGRAM_WEBHOOK=https://api.telegram.org/bot<YOUR_BOT_TOKEN>/sendMessage?chat_id=<YOUR_CHAT_ID>

# LINE Messaging API (Official Account)
# Get from LINE Developers Console
VITE_LINE_CHANNEL_ACCESS_TOKEN=your_channel_access_token_here
# Optional: Specific user ID for direct messages (leave empty for broadcast)
VITE_LINE_USER_ID=your_line_user_id

# Trading Configuration
VITE_AUTO_TRADING_ENABLED=false
VITE_RISK_PERCENTAGE=1
VITE_MAX_POSITIONS=3

# Demo Mode Override (set to true to force demo mode even with WS URL)
VITE_DEMO_MODE=false
